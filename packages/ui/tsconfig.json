{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "vueCompilerOptions": {
    "globalTypesPath": ""
  },
  "include": [
    "./**/*",
    "./vue.d.ts",
    "src",
    "types"
  ]
}